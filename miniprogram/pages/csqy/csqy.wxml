<swiper class="swiper page-section-spacing" indicator-dots="{{indicatorDots}}" autoplay="false" indicator-active-color="white">
  <block wx:for="{{imgUrls}}" wx:key="*this">
    <swiper-item>
      <image src="{{item}}" class="swiper-item" />
      <text class='swiper-text'>比赛区域</text>
    </swiper-item>
  </block>
</swiper>


<view class="page">
  <view class="page__bd">
    <view class="weui-panel">
      <view class="weui-panel__hd">比赛详情</view>
      <view class="weui-panel__bd">
        <view class="weui-media-box weui-media-box_small-appmsg">
          <view class="weui-cells weui-cells_in-small-appmsg">
            <navigator url="" bindtap='bsxq'  class="weui-cell weui-cell_access" hover-class="weui-cell_active">
              <view class="weui-cell__hd">
                <!-- <image src="{{icon20}}" style="width: 20px;height: 20px;margin-right: 5px" /> -->
              </view>
              <view class="weui-cell__bd weui-cell_primary">
                <view>比赛详情</view>
              </view>
              <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>
          </view>
        </view>
      </view>
    </view>

    <view class="weui-panel">
      <view class="weui-panel__hd">协会代表</view>
      <view class="weui-panel__bd">
        <view class="weui-media-box weui-media-box_text">
          <view class="weui-media-box__title weui-media-box__title_in-text">参会代表</view>
          <view class="weui-media-box__desc">网络电视总台:马总，艺术代表团:吉祥如意,xxxxxxxxx</view>
          <view class="weui-media-box__info">
            <view class="weui-media-box__info__meta">2019-07-05</view>
            <view class="weui-media-box__info__meta">时间</view>
            <view class="weui-media-box__info__meta weui-media-box__info__meta_extra">其它信息</view>
          </view>
        </view>
      </view>
    </view>


    <view class="weui-panel">
      <view class="weui-panel__hd">海选规则一</view>
      <view class="weui-panel__bd">
        <view class="weui-media-box weui-media-box_text">
          <view class="weui-media-box__title weui-media-box__title_in-text">一级</view>
          <view class="weui-media-box__desc">各地区办事处初选3-5个节目，由大象艺术团参与</view>
          <view class="weui-media-box__info">
            <view class="weui-media-box__info__meta">2019-07-05</view>
            <view class="weui-media-box__info__meta">时间</view>
            <view class="weui-media-box__info__meta weui-media-box__info__meta_extra">其它信息</view>
          </view>
        </view>
      </view>
    </view>


    <view class="weui-panel">
      <view class="weui-panel__hd">海选规则二</view>
      <view class="weui-panel__bd">
        <view class="weui-media-box weui-media-box_text">
          <view class="weui-media-box__title weui-media-box__title_in-text">二级</view>
          <view class="weui-media-box__desc">六大区筛选后，挑选3-5个节目；由大象艺术团参与</view>
          <view class="weui-media-box__info">
            <view class="weui-media-box__info__meta">2019-07-05</view>
            <view class="weui-media-box__info__meta">时间</view>
            <view class="weui-media-box__info__meta weui-media-box__info__meta_extra">其它信息</view>
          </view>
        </view>
      </view>
    </view>


    <view class="weui-panel">
      <view class="weui-panel__hd">海选规则三</view>
      <view class="weui-panel__bd">
        <view class="weui-media-box weui-media-box_text">
          <view class="weui-media-box__title weui-media-box__title_in-text">三级</view>
          <view class="weui-media-box__desc">综合海选分各显，专业导师评委，评选，由大象艺术团</view>
          <view class="weui-media-box__info">
            <view class="weui-media-box__info__meta">2019-07-05</view>
            <view class="weui-media-box__info__meta">时间</view>
            <view class="weui-media-box__info__meta weui-media-box__info__meta_extra">其它信息</view>
          </view>
        </view>
      </view>
    </view>

  </view>
</view>